    <script>
document.addEventListener("DOMContentLoaded", function() {
    // Modal öğeleri
    var modal = document.getElementById("filmModal");
    var btn1 = document.getElementById("openModalBtn1");
    var btn2 = document.getElementById("openModalBtn2");
    var span = document.getElementsByClassName("close")[0];

    // Eğer buton doğru şekilde seçildiyse
    if (btn1 && btn2) {
        // Butonlardan herhangi birine tıklandığında modal'ı göster
        btn1.onclick = function() {
            modal.style.display = "block";
        };
        
        btn2.onclick = function() {
            modal.style.display = "block";
        };
    }

    // Çarpı işaretine (close) tıklanınca modal'ı kapat
    span.onclick = function() {
        modal.style.display = "none";
    };

    // Modal dışına bir yere tıklanırsa modal'ı kapat
    window.onclick = function(event) {
        if (event.target === modal) {
            modal.style.display = "none";
        }
    };
});

// Diğer JavaScript fonksiyonları ve AJAX işlemi
document.addEventListener('DOMContentLoaded', function() {
    var modal = document.getElementById("filmModal");
    var openModalButton1 = document.getElementById("openModalBtn1");
    var openModalButton2 = document.getElementById("openModalBtn2");
    var closeModalButton = document.getElementsByClassName("close")[0];
    var successMessage = document.getElementById("successMessage");
    var form = document.getElementById("filmForm");
    var filmAdiInput = document.getElementById("film_adi");
    var kullaniciAdiInput = document.getElementById("kullanici_adi");
    var recaptcha = document.getElementById("recaptcha");
    var basliksil = document.getElementById("basliksil");

    // Modal'ı açmak için her iki butona tıklama işlemi
    openModalButton1.addEventListener("click", function() {
        modal.style.display = "block";
    });

    openModalButton2.addEventListener("click", function() {
        modal.style.display = "block";
    });

    // Modal'ı kapatmak için × butonuna tıklama
    closeModalButton.addEventListener("click", function() {
        modal.style.display = "none";
        successMessage.style.display = "none"; // Başarı mesajını gizle
    });

    // Modal dışına tıklanırsa modal'ı kapat
    window.addEventListener("click", function(event) {
        if (event.target === modal) {
            modal.style.display = "none";
            successMessage.style.display = "none"; // Başarı mesajını gizle
        }
    });

    // Form gönderildiğinde AJAX ile veri gönderme işlemi
    form.addEventListener("submit", function(event) {
        event.preventDefault(); // Sayfanın yenilenmesini engelle

        // reCAPTCHA doğrulamasını kontrol et
        var recaptchaResponse = grecaptcha.getResponse();
        if (recaptchaResponse.length === 0) {
            alert("Lütfen CAPTCHA doğrulamasını tamamlayın.");
            return; // CAPTCHA tamamlanmamışsa form gönderilmesin
        }

        // Form verilerini al
        var filmAdi = filmAdiInput.value;
        var kullaniciAdi = kullaniciAdiInput.value;

        // AJAX isteği oluştur
        var baseUrl = "https://dizifun2.com/";  // PHP'den dinamik URL'yi doğru al
        var xhr = new XMLHttpRequest();
        xhr.open("POST", baseUrl + "istek_gonder.php", true);  // URL'yi doğru birleştirin
        xhr.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");

        // Verileri gönder
        var data = "film_adi=" + encodeURIComponent(filmAdi) + "&kullanici_adi=" + encodeURIComponent(kullaniciAdi) + "&recaptcha_response=" + encodeURIComponent(recaptchaResponse);
        xhr.send(data);

        // AJAX yanıtını işle
        xhr.onreadystatechange = function() {
            if (xhr.readyState === 4 && xhr.status === 200) {
                console.log(xhr.responseText); // PHP yanıtını kontrol et
                if (xhr.responseText.trim() === "success") {
                    // Başarı durumunda inputları, reCAPTCHA'yı ve label'ları gizle
                    filmAdiInput.style.display = "none"; 
                    kullaniciAdiInput.style.display = "none"; 
                    recaptcha.style.display = "none"; // reCAPTCHA'yı gizle
                    basliksil.style.display = "none"; // Başlık sil butonunu gizle
                    form.querySelector('button[type="submit"]').style.display = "none"; // Gönder butonunu gizle
                    
                    // Label'ları da gizle
                    form.querySelector('label[for="film_adi"]').style.display = "none";
                    form.querySelector('label[for="kullanici_adi"]').style.display = "none";

                    successMessage.style.display = "block"; // Başarı mesajını göster
                    form.reset(); // Formu sıfırla (inputs boş kalacak)
                    
                    // reCAPTCHA'yı sıfırlamak
                    grecaptcha.reset();
                } else {
                    // Hata durumunda kullanıcıya bildirim göster
                    alert("Bir hata oluştu: " + xhr.responseText);
                }
            }
        };
    });
});

    </script>
<!-- Google reCAPTCHA API -->
