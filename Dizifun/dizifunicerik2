<script>
function openStoriesModal(imageUrl, title, linkUrl) {
  const modal = document.getElementById('stories-modal');
  const modalImage = document.getElementById('stories-modal-image');
  const modalTitle = document.getElementById('stories-modal-title');
  const modalLink = document.getElementById('stories-modal-link');

  modalImage.src = imageUrl;
  modalTitle.textContent = title;
  modalLink.href = linkUrl;

  modal.style.display = 'flex';
}

function closeStoriesModal() {
  const modal = document.getElementById('stories-modal');
  modal.style.display = 'none';
}

// Modal kapama için dışarıya tıklama kontrolü
window.onclick = function(event) {
  const modal = document.getElementById('stories-modal');
  if (event.target === modal) {
    modal.style.display = 'none';
  }
};

// Dinamik kontrol: Süresi dolan story'leri gizle
setInterval(() => {
  const now = new Date();
  document.querySelectorAll('.story').forEach(story => {
    const endTime = new Date(story.dataset.endTime); // end_time HTML5 data attribute'unda olmalı
    if (endTime <= now) {
      story.style.display = 'none'; // Süresi dolmuş story'yi gizle
    }
  });
}, 1000); // Her saniye kontrol et

</script>







