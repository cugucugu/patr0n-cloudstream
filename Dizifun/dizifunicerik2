<script>
    // Çerez kontrolü yaparak uyarıyı gösterecek fonksiyon
    function checkAndShowAlert() {
        // Bugünün tarihini al
        var today = new Date().toISOString().split('T')[0]; // YYYY-MM-DD formatında

        // Kullanıcının daha önce o gün uyarıyı görüp görmediğini kontrol et
        if (getCookie("duyuruGoruldu") !== today) {
            // Uyarıyı göster
            document.getElementById("duyuru").style.display = "block";
            
            // Uyarıyı gördüğünü belirten ve bugünün tarihini içeren çerezi oluştur
            setCookie("duyuruGoruldu", today, 1); // 1 gün geçerli
        }
    }

    // Çerez ayarlama fonksiyonu
    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }

    // Çerezi okuma fonksiyonu
    function getCookie(cname) {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }

    // Sayfa yüklendiğinde kontrol et
    window.onload = checkAndShowAlert;
</script>

