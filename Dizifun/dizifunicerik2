    <script>
        // Sağ tıklama engelleme
        document.addEventListener('contextmenu', function(e) {
            e.preventDefault();
        });

        // Kısayol tuşlarını engelleme
        document.addEventListener('keydown', function(e) {
            if (e.key === 'F12' || 
                (e.ctrlKey && e.shiftKey && (e.key === 'I' || e.key === 'J')) || 
                (e.ctrlKey && e.key === 'U')) {
                e.preventDefault();
            }
        });

        // DevTools kontrolü
        let devtoolsOpen = false;
        const overlay = document.createElement('div');
        overlay.className = 'devtools-overlay';
        overlay.innerHTML = '<div class="devtools-message">Lütfen geliştirici araçlarını kapatın!</div>';
        document.body.appendChild(overlay);

        setInterval(() => {
            const isDevToolsOpen = (window.outerWidth - window.innerWidth > 160) || 
                                  (window.outerHeight - window.innerHeight > 160);
            if (isDevToolsOpen && !devtoolsOpen) {
                devtoolsOpen = true;
                overlay.style.display = 'flex'; // Uyarıyı göster
            } else if (!isDevToolsOpen && devtoolsOpen) {
                devtoolsOpen = false;
                overlay.style.display = 'none'; // Uyarıyı gizle
                window.location.reload(); // Sayfayı yenile
            }
        }, 500);
    </script>
