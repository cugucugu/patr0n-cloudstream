<script>

function hexToString(hex) {
    let str = '';
    for (let i = 0; i < hex.length; i += 2) {
        str += String.fromCharCode(parseInt(hex.substr(i, 2), 16));
    }
    return str;
}


function changePlayer(playerType) {
    let playerUrl;
    if (playerType === "jwplayer") {
        playerUrl = decodeURIComponent(hexToString("2f2f706c6179686f7573652e7072656d69756d766964656f2e636c69636b2f706c617965722f6d69726470336a683963366f6238763579746838"));
    } else if (playerType === "videojs") {
        playerUrl = decodeURIComponent(hexToString("2f2f706c6179686f7573652e7072656d69756d766964656f2e636c69636b2f61726d6f6e792f6d69726470336a683963366f6238763579746838"));
    }

    // iframe'in src'sini güncelle
    document.getElementById('playerFrame').src = playerUrl;
}

// Sayfa yüklendiğinde varsayılan player URL'sini ata (jwplayer)
document.getElementById('playerFrame').src = decodeURIComponent(hexToString("2f2f706c6179686f7573652e7072656d69756d766964656f2e636c69636b2f706c617965722f6d69726470336a683963366f6238763579746838"));
</script>



</div>

